@model EntityLayer.Concrete.Message



@{
    ViewData["Title"] = "MessageDetails";
    Layout = "~/Views/Shared/WriterLayout.cshtml";
}


<h1>MessageDetails</h1>
<br />

@Html.Label("Message ID")
@Html.TextBoxFor(x=> x.MessageID, new{@class= "form-control",@readonly="readonly"} )  @*@ReadOnly kismi textboxin editidini iptal etmek icin kullanildi*@
<br />
@Html.Label("Subject")
@Html.TextBoxFor(x=> x.Subject, new{@class= "form-control",disable="disabled"} )
<br />
@Html.Label("Message")
@Html.TextAreaFor(x=> x.MessageDetails,10,3, new{@class= "form-control", disabled = "disabled"} ) @*Disable da ayni sebep icin kullanildi*@
<br />

<br />
<a href="/Message/InBox/" class="btn btn-primary">Return To Messages</a>



